{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "![gif-crud](https://wingedseraph.github.io/dump/assets/crud.gif)",
  "main": "index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "start:prod": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "start:multi": "cross-env NODE_ENV=multi ts-node src/cluster.ts",
    "build": "webpack --mode production",
    "test": "jest --detectOpenHandles --forceExit",
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedseraph/crud-api.git"
  },
  "engines": {
    "node": ">=24.14.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/wingedseraph/crud-api/issues"
  },
  "homepage": "https://github.com/wingedseraph/crud-api#readme",
  "dependencies": {
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.1.4"
  }
